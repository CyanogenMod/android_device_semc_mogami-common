    capslock:                           'i'
    shift+capslock:                     'I'
    alt, alt+capslock:                  '8'
}

key O {
    label:                              'O'
    base:                               '\u03BF'
    shift:                              '\u039F'
    capslock:                           'o'
    shift+capslock:                     'O'
