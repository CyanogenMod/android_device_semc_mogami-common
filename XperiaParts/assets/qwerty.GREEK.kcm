#
# QWERTY keyboard layout.
# For Sony Ericsson Xperia Mini Pro and Sony Ericsson Xperia Pro.
# Made by hnl_dk.
#

type ALPHA

### ROW 1

key Q {
    label:                              'Q'
    base:                               '\\'
    shift:                              '\\'
    capslock:                           'q'
    shift+capslock:                     'Q'
    alt, alt+capslock:                  '1'
}

key W {
    label:                              'W'
    base:                               '\u03C2'
    shift:                              '\u03C2'
    capslock:                           'w'
    shift+capslock:                     'W'
    alt, alt+capslock:                  '2'
}

key E {
    label:                              'E'
    base:                               '\u03B5'
    shift:                              '\u0395'
    capslock:                           'e'
    shift+capslock:                     'E'
    alt, alt+capslock:                  '3'
}

key R {
    label:                              'R'
    base:                               '\u03C1'
    shift:                              '\u03A1'
    capslock:                           'r'
    shift+capslock:                     'R'
    alt, alt+capslock:                  '4'
}

key T {
    label:                              'T'
    base:                               '\u03C4'
    shift:                              '\u03A4'
    capslock:                           't'
    shift+capslock:                     'T'
    alt, alt+capslock:                  '5'
}

key Y {
    label:                              'Y'
    base:                               '\u03C5'
    shift:                              '\u03A5'
    capslock:                           'y'
    shift+capslock:                     'Y'
